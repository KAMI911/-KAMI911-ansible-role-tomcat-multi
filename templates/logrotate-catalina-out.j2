# {{ ansible_managed }}
{% for instance in tomcat_instance %}
{{ tomcat_system_home }}{{ instance.instance_name }}/logs/catalina/catalina.out {
  su {{ tomcat_system_user }} {{ tomcat_system_group }}
  daily
  rotate 730
  copytruncate
  compress
  delaycompress
  missingok
  notifempty
  compresscmd /usr/bin/xz
  compressoptions "-9e"
  compressext .xz
}

{% endfor %}
